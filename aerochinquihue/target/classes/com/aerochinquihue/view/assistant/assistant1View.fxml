<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<BorderPane id="datosPane" prefHeight="363.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.aerochinquihue.controller.assistantController">
   <bottom>
      <HBox alignment="CENTER_LEFT" prefHeight="71.0" prefWidth="600.0" spacing="15.0" BorderPane.alignment="CENTER">
         <children>
            <Button mnemonicParsing="false" onAction="#handleAssistant0View" text="Volver">
               <font>
                  <Font size="19.0" />
               </font>
            </Button>
            <VBox alignment="CENTER_LEFT" prefHeight="71.0" prefWidth="357.0">
               <children>
                  <Label text="Direccion:">
                     <font>
                        <Font size="18.0" />
                     </font>
                  </Label>
                  <TextField fx:id="TextFieldDireccion" prefHeight="25.0" prefWidth="348.0" />
               </children>
            </VBox>
            <Button layoutX="25.0" layoutY="26.0" mnemonicParsing="false" onAction="#handleAssistant2View" text="Continuar">
               <font>
                  <Font size="19.0" />
               </font>
            </Button>
         </children>
         <padding>
            <Insets left="15.0" />
         </padding>
      </HBox>
   </bottom>
   <top>
      <HBox alignment="CENTER" prefHeight="68.0" prefWidth="600.0" BorderPane.alignment="CENTER">
         <children>
            <Label text="Ingrese sus datos:">
               <font>
                  <Font size="20.0" />
               </font>
            </Label>
         </children>
      </HBox>
   </top>
   <left>
      <VBox alignment="CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="261.0" prefWidth="300.0" spacing="5.0" BorderPane.alignment="CENTER">
         <children>
            <Label text="Nombre:">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
            <TextField fx:id="TextFieldNombre" prefHeight="25.0" prefWidth="348.0" promptText="ejemplo ejemplo2">
               <font>
                  <Font size="15.0" />
               </font>
            </TextField>
            <Label layoutX="25.0" layoutY="10.0" text="Apellidos:">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
            <TextField fx:id="TextFieldApellidos" layoutX="25.0" layoutY="42.0" prefHeight="25.0" prefWidth="348.0" promptText="ejemplo3 ejemplo4">
               <font>
                  <Font size="15.0" />
               </font>
            </TextField>
         </children>
         <padding>
            <Insets left="15.0" right="15.0" />
         </padding>
      </VBox>
   </left>
   <right>
      <VBox alignment="CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="261.0" prefWidth="300.0" spacing="5.0" BorderPane.alignment="CENTER">
         <padding>
            <Insets left="15.0" right="15.0" />
         </padding>
         <children>
            <Label text="Telefono:">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
            <TextField fx:id="TextFieldTelefonos" prefHeight="25.0" prefWidth="348.0" promptText="+56 9 1234 5678">
               <font>
                  <Font size="15.0" />
               </font>
            </TextField>
            <Label layoutX="25.0" layoutY="10.0" text="Rut:">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
            <TextField fx:id="TextFieldRut" layoutX="25.0" layoutY="42.0" prefHeight="25.0" prefWidth="348.0" promptText="12345678-k">
               <font>
                  <Font size="15.0" />
               </font>
            </TextField>
         </children>
      </VBox>
   </right>
</BorderPane>
